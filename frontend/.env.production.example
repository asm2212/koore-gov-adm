# Production Frontend Environment Variables

# API Configuration
NEXT_PUBLIC_API_URL=https://api.your-domain.com/api

# Application Configuration
NEXT_PUBLIC_APP_NAME="Koore Government Administration"
NEXT_PUBLIC_APP_VERSION=1.0.0

# Analytics and Monitoring (optional)
# NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
# NEXT_PUBLIC_SENTRY_DSN=your-sentry-dsn

# Feature Flags (optional)
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_DEBUG=false

# Contact Information
NEXT_PUBLIC_CONTACT_EMAIL=info@your-domain.com
NEXT_PUBLIC_CONTACT_PHONE=+251-xxx-xxx-xxxx

# Social Media Links (optional)
# NEXT_PUBLIC_FACEBOOK_URL=https://facebook.com/your-page
# NEXT_PUBLIC_TWITTER_URL=https://twitter.com/your-handle
# NEXT_PUBLIC_LINKEDIN_URL=https://linkedin.com/company/your-company
